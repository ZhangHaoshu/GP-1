/* components/mysearch/mysearch.wxss */
.my-search-container {
    background-color: #ffffff;
    height: 100rpx;
    padding: 0 10rpx;
    display: flex;
    align-items: center;
}

.my-search-box {
    height: 60rpx;
    background-color: #eceaea;
    border-radius: 15rpx;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.placeholder {
    font-size: 15px;
    margin-left: 5px;
}