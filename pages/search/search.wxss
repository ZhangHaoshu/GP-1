/* pages/search/search.wxss */
.sugg-list {
    padding: 0 5px;
}
.sugg-item {
    font-size: 15px;
    padding: 13px 0;
    border-bottom: 1px solid #efefef;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.device-name {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-right: 3px;
}